{
  "name": "xitickets-frontend",
  "version": "0.0.0",
  "description": "Open source theater ticketing and seating chart application",
  "author": {
    "name": "Jared Bates",
    "email": "me@jaredbates.net",
    "url": "https://jaredbates.net"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/XiTickets/Frontend.git"
  },
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" \"npm run sass:w\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "sass:w": "node node_modules\\node-sass\\bin\\node-sass -w styles -o styles",
    "postinstall": "typings install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "angular2-in-memory-web-api": "0.0.18",
    "angular2-localstorage": "^0.4.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.0",
    "ng2-bootstrap": "1.1.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "0.6.17"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "node-sass": "^3.8.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}

  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/computerwizjared/TheatreTickets.git"
  },
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" \"npm run sass:w\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "sass:w": "node node_modules\\node-sass\\bin\\node-sass -w styles -o styles",
    "postinstall": "typings install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
